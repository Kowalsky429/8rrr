{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": ["next/babel", "next/core-web-vitals", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": "warn",
    "eqeqeq": "warn",
    "no-unused-vars": "warn"
  }
}
